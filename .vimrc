set number	
syntax on
set relativenumber
set showmatch
set hlsearch
set smartcase
set ignorecase 
set autoindent
set expandtab
set shiftwidth=4
set smartindent
set smarttab
set softtabstop=4
set ruler 
set undolevels=1000
set backspace=indent,eol,start
# Enter normal mode with STRG + D
inoremap <C-d> <Esc>
